{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/hello1",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/gateway/hello1",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "1"
    },
    {
      "DownstreamPathTemplate": "/api/hello2",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5002
        }
      ],
      "UpstreamPathTemplate": "/gateway/hello2",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "2"
    }
  ],

  "SwaggerEndPoints": [
    {
      "Key": "1",
      "Config": [
        {
          "Name": "API Client",
          "Version": "v1",
          "Url": "http://localhost:5001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "2",
      "Config": [
        {
          "Name": "API Manager",
          "Version": "v1",
          "Url": "http://localhost:5002/swagger/v1/swagger.json"
        }
      ]
    }
  ],

  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}
